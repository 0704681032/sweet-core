{
  "name": "sweet.js",
  "description": "Hygienic Macros for JavaScript",
  "main": "lib/sweet.js",
  "version": "0.10.0-alpha.0",
  "engines": {
    "node": ">=0.4.0"
  },
  "author": "Tim Disney",
  "licenses": [
    {
      "type": "BSD",
      "url": "http://github.com/mozilla/sweet.js/master/LICENSE.BSD"
    }
  ],
  "scripts": {
    "clean": "rm -rf build/",
    "build:src": "babel --modules common --source-maps -d build/ src/*.js",
    "build:test": "babel --modules common --source-maps -d build/ test/*.js",
    "build": "npm run build:src && npm run build:test",
    "test:single": "mocha build/test/test_expander.js",
    "test": "mocha build/test/*.js",
    "pretest": "npm run build"
  },
  "directories": {
    "test": "test"
  },
  "bin": {
    "sjs": "bin/sjs"
  },
  "dependencies": {
    "escodegen": "1.6.x",
    "escope": "1.0.x",
    "optimist": "~0.3.4",
    "resolve": "~0.6.1",
    "underscore": "~1.3.3"
  },
  "devDependencies": {
    "babel": "^5.8.9",
    "contracts-js": "^0.6.0",
    "escodegen": "^1.6.1",
    "esfuzz": "disnet/esfuzz",
    "expect.js": "0.1.x",
    "grunt-cli": "0.1.x",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-contrib-jshint": "~0.7.2",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-mocha-test": "~0.7.0",
    "harmony-reflect": "^1.0.0",
    "immutable": "^3.7.4",
    "mocha": "^2.2.5",
    "source-map": "~0.1.31",
    "tape": "^4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mozilla/sweet.js.git"
  },
  "keywords": [
    "macros",
    "javascript"
  ],
  "license": "BSD"
}
