{
  "name": "sweet.js",
  "description": "Hygienic Macros for JavaScript",
  "main": "index.js",
  "bin": {
    "sjs": "bin/sjs"
  },
  "version": "1.0.0-alpha.15",
  "engines": {
    "node": ">4.0.0"
  },
  "author": "Tim Disney",
  "licenses": [
    {
      "type": "BSD",
      "url": "http://github.com/mozilla/sweet.js/master/LICENSE.BSD"
    }
  ],
  "scripts": {
    "test": "node_modules/.bin/ava",
    "curr": "node_modules/.bind/ava test/test_macro_expansion.js",
    "smoke": "node_modules/.bin/ava test/*.js !test/assertions.js",
    "smoke:watch": "node_modules/.bin/ava test/*.js !test/assertions.js --watch",
    "report": "node_modules/.bin/nyc node_modules/.bin/ava && node_modules/.bin/nyc report --reporter=html",
    "docs": "node_modules/.bin/gitbook build doc/book/",
    "prepublish": "gulp dist"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "babel-core": "^6.5.0",
    "immutable": "^3.7.4",
    "ramda": "^0.19.0",
    "ramda-fantasy": "^0.4.1",
    "resolve": "^0.6.3",
    "shift-codegen": "^4.0.0",
    "shift-js": "^0.2.1",
    "shift-parser": "^4.1.0",
    "shift-reducer": "^3.0.2",
    "shift-spidermonkey-converter": "^1.0.0",
    "transit-js": "^0.8.846",
    "yargs": "^3.31.0"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel": "^6.5.0",
    "babel-cli": "^6.5.0",
    "babel-plugin-syntax-flow": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "expect.js": "0.1.x",
    "gitbook-cli": "^1.0.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-webpack": "^1.5.0",
    "nyc": "^6.0.0",
    "source-map": "~0.1.31",
    "source-map-support": "^0.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mozilla/sweet.js.git"
  },
  "keywords": [
    "macros",
    "javascript"
  ],
  "license": "BSD-2-Clause",
  "ava": {
    "files": [
      "test/*.js",
      "test/declaration/*.js",
      "test/destructuring/**/*.js",
      "test/expressions/**/*.js",
      "test/modules/**/*.js",
      "test/statements/**/*.js",
      "!test/assertions.js"
    ],
    "require": [
      "babel-register"
    ]
  }
}
