{
  "name": "sweet.js",
  "description": "Hygienic Macros for JavaScript",
  "main": "lib/sweet.js",
  "version": "0.10.0-alpha.0",
  "engines": {
    "node": ">4.0.0"
  },
  "author": "Tim Disney",
  "licenses": [
    {
      "type": "BSD",
      "url": "http://github.com/mozilla/sweet.js/master/LICENSE.BSD"
    }
  ],
  "scripts": {
    "clean": "rm -rf build/",
    "build:src": "node_modules/.bin/babel --source-maps -d build/ src/*.js",
    "build:test": "node_modules/.bin/babel --source-maps -d build/ test/*.js",
    "build": "npm run clean && npm run build:src && npm run build:test",
    "test:single": "mocha build/test/test_expression_parsing.js",
    "test": "mocha --reporter dot --require source-map-support/register build/test/*.js",
    "pretest": "npm run build"
  },
  "directories": {
    "test": "test"
  },
  "bin": {
    "sjs": "bin/sjs"
  },
  "dependencies": {
    "optimist": "~0.3.4",
    "resolve": "~0.6.1",
    "transit-immutable-js": "^0.4.0",
    "transit-js": "^0.8.846"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.15",
    "babel-plugin-syntax-flow": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "core-js": "^1.2.6",
    "eslint-plugin-flowtype": "^1.0.0",
    "expect.js": "0.1.x",
    "immutable": "^3.7.4",
    "mocha": "^2.2.5",
    "ramda": "^0.19.0",
    "ramda-fantasy": "^0.4.1",
    "shift-codegen": "^3.0.0",
    "shift-js": "^0.2.1",
    "shift-parser": "^3.0.5",
    "shift-reducer": "^2.0.4",
    "shift-spidermonkey-converter": "^1.0.0",
    "source-map": "~0.1.31",
    "source-map-support": "^0.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mozilla/sweet.js.git"
  },
  "keywords": [
    "macros",
    "javascript"
  ],
  "license": "BSD"
}
