{
  "name": "sweet.js",
  "description": "Hygienic Macros for JavaScript",
  "main": "dist/sweet.js",
  "bin": {
    "sjs": "bin/sjs"
  },
  "version": "1.0.0-pre.3",
  "engines": {
    "node": ">4.0.0"
  },
  "author": "Tim Disney",
  "licenses": [
    {
      "type": "BSD",
      "url": "http://github.com/mozilla/sweet.js/master/LICENSE.BSD"
    }
  ],
  "scripts": {
    "test": "node_modules/.bin/gulp build && node_modules/.bin/ava",
    "curr": "node_modules/.bin/ava build/test/modules/export.js",
    "smoke": "node_modules/.bin/ava build/test/*.js !build/test/assertions.js",
    "smoke:watch": "node_modules/.bin/ava build/test/*.js !build/test/assertions.js --watch",
    "report": "node_modules/.bin/nyc node_modules/.bin/ava && node_modules/.bin/nyc report --reporter=html",
    "docs": "node_modules/.bin/asciidoctorjs doc/1.0/tutorial.adoc && node_modules/.bin/asciidoctorjs doc/1.0/reference.adoc",
    "prepublish": "gulp dist"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "babel-core": "^6.5.0",
    "immutable": "^3.7.4",
    "ramda": "^0.19.0",
    "ramda-fantasy": "^0.4.1",
    "resolve": "^1.1.7",
    "shift-codegen": "^4.0.0",
    "shift-js": "^0.2.1",
    "shift-parser": "^4.1.0",
    "shift-reducer": "^3.0.2",
    "shift-spidermonkey-converter": "^1.0.0",
    "transit-js": "^0.8.846",
    "yargs": "^4.3.2"
  },
  "devDependencies": {
    "asciidoctor-cli": "^1.5.2-alpha.3",
    "asciidoctor.js": "^1.5.4",
    "ava": "^0.13.0",
    "babel": "^6.5.0",
    "babel-cli": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "event-stream": "^3.3.2",
    "expect.js": "0.3.x",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "gulp-webpack": "^1.5.0",
    "merge": "^1.2.0",
    "merge-stream": "^1.0.0",
    "nyc": "^6.0.0",
    "source-map": "~0.5.3",
    "source-map-support": "^0.4.0",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mozilla/sweet.js.git"
  },
  "keywords": [
    "macros",
    "javascript"
  ],
  "license": "BSD-2-Clause",
  "ava": {
    "files": [
      "build/test/*.js",
      "build/test/declaration/*.js",
      "build/test/destructuring/**/*.js",
      "build/test/expressions/**/*.js",
      "build/test/modules/**/*.js",
      "build/test/statements/**/*.js",
      "!build/test/assertions.js"
    ]
  }
}
